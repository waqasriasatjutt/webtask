<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <!--Adding New Sequence For A Field-->
    <!--  https://www.youtube.com/watch?v=ZfKzmfiqeg0&list=PLqRRLx0cl0hoJhjFWkFYowveq2Zn55dhM&index=8  -->
    <data noupdate="1">
        <record id="seq_hospital_patient" model="ir.sequence">
            <field name="name">patient Sequence</field>
            <field name="code">hospital.patient.sequence</field>
            <field name="prefix">P</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>


    </data>
</odoo>
